{
  "name": "electron-react-admin",
  "author": "shenhaoxiang",
  "version": "1.0.0",
  "description": "electron + react 模板",
  "main": "dist/main/main.js",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run dev:ui\" \"npm run dev:main\"",
    "dev:ui": "cross-env NODE_ENV=development webpack-dev-server --config config/ui/webpack.dev.js",
    "dev:main": "cross-env NODE_ENV=development electron-webpack dev",
    "build:ui": "webpack --config config/ui/webpack.prod.js",
    "build:main": "webpack --config config/main/webpack.prod.js",
    "release:dev": "cross-env NODE_ENV=development yarn build:main && cross-env NODE_ENV=development yarn build:ui && yarn build",
    "release:prod": "cross-env NODE_ENV=production yarn build:main && cross-env NODE_ENV=production yarn build:ui && yarn build",
    "build": "electron-builder --ia32 --config electron-builder.json",
    "postinstall": "electron-builder install-app-deps",
    "clean": "yarn autoclean -I && yarn autoclean -F"
  },
  "keywords": [
    "Electron",
    "react",
    "typescript"
  ],
